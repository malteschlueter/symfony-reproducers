{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://github.com/malteschlueter/symfony-serializer",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/flex": "^1.3.1",
        "symfony/property-info": "5.3.*",
        "symfony/serializer": "dev-bugfix/name-converter"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "symfony/phpunit-bridge": "5.3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
